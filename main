#include <iostream>
#include <string>
#include <iomanip>
using namespace std;

int main() {
    //declare array to store names
    string array[3];

    //ask user for names
    cout << "Enter name 1:";
    cin >> array[0];

    cout << "Enter name 2:";
    cin >> array[1];

    cout << "Enter name 3:";
    cin >> array[2];

    //print the first name
    cout << array[0];

    return 0;
}
